import React, { Component } from 'react';
import Types from 'prop-types';

export default class <%= name %> extends Component {
  render() {
    const {
    <% variables.forEach(function(variable) { %>
      <%= variable.name %>,
    <% }) %>
    } = this.props;

    return <h1>Hello, world!</h1>;
  };
};

<%= name %>.propTypes = {
  <% variables.forEach(function(variable) { %>
    <%= variable.name %>: Types.<%= variable.value %>,
  <% }) %>
}
